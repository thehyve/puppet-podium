{
  "name": "thehyve-podium",
  "version": "0.0.1",
  "author": "thehyve",
  "summary": "Puppet module for the Podium request portal",
  "license": "GPL-3.0",
  "source": "https://github.com/thehyve/puppet-podium",
  "project_page": "https://github.com/thehyve/puppet-podium",
